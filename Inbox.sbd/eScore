14/06/2014.

Ch=C6=B0=C6=A1ng tr=C3=ACnh d=E1=BB=B1 ki=E1=BA=BFn nh=C6=B0 sau:

*	T=E1=BB=95 ch=E1=BB=A9c 1 tr=E1=BA=ADn b=C3=B3ng =C4=91=C3=A1.
*	Nh=E1=BA=ADu nh=E1=BA=B9t =C4=83n ch=C6=A1i =C4=91=C3=A0n =
=C4=91=C3=BAm: =E1=BB=9F H=C3=A0 t=C3=A2y (d=E1=BB=B1 ki=E1=BA=BFn =
Thi=C3=AAn s=C6=A1n su=E1=BB=91i ng=C3=A0)
*	H=E1=BB=99i thao: n=E1=BA=BFu c=C3=B2n =C4=91=E1=BB=A7 s=E1=BB=A9c
*	Nh=E1=BA=ADu nh=E1=BA=B9t, ch=C3=A9m gi=C3=B3 v=C3=A0 xem b=C3=B3ng =
=C4=91=C3=A1.

Ph=C3=A2n c=C3=B4ng nhi=E1=BB=87m v=E1=BB=A5 nh=C6=B0 sau:

*	Kh=E1=BA=A3o s=C3=A1t =C4=91=E1=BB=8Ba =C4=91i=E1=BB=83m: S=C6=A1nTD =
trong tu=E1=BA=A7n n=C3=A0y- b=C3=A1o c=C3=A1o k=E1=BA=BFt qu=E1=BA=A3 =
tr=C6=B0=E1=BB=9Bc th=E1=BB=A9 2 tu=E1=BA=A7n sau.
*	Thu=C3=AA ph=C6=B0=C6=A1ng ti=E1=BB=87n: D=C6=B0=C6=A1ng qu=C3=A1- =
thu=C3=AA xe =C3=B4 t=C3=B4 =C4=91=E1=BB=83 ch=E1=BB=9F =C4=91=E1=BB=93.
*	K=E1=BB=B7 ni=E1=BB=87m ch=C6=B0=C6=A1ng: Ti=E1=BA=BFn Anh- =C4=91i =
soi lu=C3=B4n trong h=C3=B4m nay v=C3=A0 ng=C3=A0y mai.
*	Th=C3=A0nh ph=E1=BA=A7n tham gia: t=C3=B4i c=C3=B3 g=E1=BB=ADi danh =
s=C3=A1ch chia l=C3=A0m c=C3=A1c ph=C3=B2ng- nh=E1=BB=AFng =
=C4=91=E1=BB=93ng ch=C3=AD b=C3=B4i m=C3=A0u xanh ch=E1=BB=8Bu =
tr=C3=A1ch nhi=E1=BB=87m tr=E1=BA=A3 l=E1=BB=9Di v=E1=BB=81 s=E1=BB=91 =
l=C6=B0=E1=BB=A3ng =C4=91=C4=83ng k=C3=BD c=E1=BB=A7a ph=C3=B2ng =
m=C3=ACnh. C=C3=A1c anh em =C4=91=C3=A3 r=E1=BB=9Di Elcom- Th=C3=A0nh TT =
s=E1=BA=BD tr=E1=BB=B1c ti=E1=BA=BFp call =C4=91=E1=BB=83 tr=C3=A1nh =
vi=E1=BB=87c lost th=C3=B4ng tin

Th=E1=BB=A9 2 tu=E1=BA=A7n t=E1=BB=9Bi, sau khi c=C3=B3 =C4=91=E1=BA=A7y =
=C4=91=E1=BB=A7 th=C3=B4ng tin s=E1=BA=BD ti=E1=BA=BFn h=C3=A0nh =
c=C3=A1c b=C6=B0=E1=BB=9Bc ti=E1=BA=BFp theo li=C3=AAn quan t=E1=BB=9Bi =
Ch=C6=B0=C6=A1ng tr=C3=ACnh c=E1=BB=A5 th=E1=BB=83, chi =
ph=C3=AD=E2=80=A6.

