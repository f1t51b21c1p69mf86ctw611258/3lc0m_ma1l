tr=E1=BA=ADn ti=E1=BB=81n t=E1=BA=A5n c=C3=B4ng v=C3=A0o th=C3=A0nh n=C6=B0=E1=BB=9Bc Kim. L=E1=BA=A7n l=C6=B0=E1=BB=A3t c=C3=A1c t=C6=B0=E1=BB=
=9Bng Vi=E1=BB=87t nh=C3=A0u, Nam d=C3=AA
ma, Tr=C6=B0=E1=BB=9Dng h=C3=B3i d=E1=BA=ABn binh c=C3=B4ng th=C3=A0nh, s=E1=BB=B9 kh=C3=AD qu=C3=A2n T=C3=A2y H=E1=BA=A1 =C4=91=E1=BA=A1=
i th=E1=BB=8Bnh. Nh=C6=B0ng
ng=C6=B0=E1=BB=9Di t=C3=ADnh kh=C3=B4ng b=E1=BA=B1ng tr=E1=BB=9Di t=C3=ADnh, binh s=E1=BB=B9 n=C6=B0=E1=BB=9Bc Kim =C4=91=E1=BB=93ng l=C3=
=B2ng anh d=C5=A9ng chi=E1=BA=BFn
=C4=91=E1=BA=A5u =C4=91=E1=BA=A9y lui h=E1=BA=BFt =C4=91=E1=BB=A3t c=C3=B4ng th=C3=A0nh n=C3=A0y =C4=91=E1=BA=BFn =C4=91=E1=BB=A3t c=C3=B4ng th=C3=A0nh=
 kh=C3=A1c c=E1=BB=A7a T=C3=A2y h=E1=BA=A1, khi=E1=BA=BFn
cho h=E1=BB=95 t=C6=B0=E1=BB=9Bng Linh tr=C3=A2u v=C3=B4 c=C3=B9ng t=E1=BB=A9c gi=E1=BA=ADn. =C4=90=E1=BB=99t nhi=C3=AAn, khi qu=C3=
